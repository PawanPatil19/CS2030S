set nocompatible
filetype on
filetype plugin on
filetype indent on
set linebrak
set wrapmargin=3
set number
set expandtab
set tabstop=2
set shiftwidth=2
set autoindent
set smartindent
set showcmd
set backspace=indent,eol,start
set writebackup
set backup
set backupdir=~/.backup
set visualbell
set scrolloff=1
set ruler
set laststatus=2
set whichwrap=b,s,h,l,<,>,[,]
set nojoinspaces
set comments=sr:/*,mb:*,el:*/,://,b:#,:%,:XCOMM,n:>,fb:-,n:\:
set matchpairs+=<:>
set clipboard=autoselect,exclude:.*
set formatoptions+=r
syntax on
set guifont=LiberationMono\ 12
syntax enable
let g:rainbow_active = 1
set wrap
set mouse=a
inoremap ( ()<Left>
inoremap [ []<Left>
inoremap { {}<Left>
inoremap ' ''<Left>
inoremap " ""<Left>
set viminfo='100,<50,s10,h
ia psvm public static void main(String[] args) {<CR>}<UP><END>
ia sout System.out.println{<CR>}
ia sf scanf

